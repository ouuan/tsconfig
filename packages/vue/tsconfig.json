{
  "extends": "@ouuan/tsconfig-base/tsconfig.json",
  "display": "ouuan's tsconfig for Vue",
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ]
  },
  "include": [
    "${configDir}/*.ts",
    "${configDir}/src/**/*.ts",
    "${configDir}/src/**/*.vue"
  ]
}
